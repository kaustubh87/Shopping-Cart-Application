{{# each products}}
    <div class="row">
        {{# each this}}
              <div class="col-sm-6 col-md-4">
                <div class="thumbnail">
                  <img src="{{this.imageSrc}}" alt="..." class="img-def">
                  <div class="caption">
                    <h3>{{this.title}}</h3>
                    <p class="description">{{this.description}}</p>
                    <div class="clearfix">
                       <div class="price pull-left">$12</div>
                        <a href="/add-to-cart/" class="btn btn-success pull-right" role="button">Add to Cart</a>
                    </div>
                  </div>
                </div>
              </div>
        {{/each}}
    </div>
{{/each}}
   


